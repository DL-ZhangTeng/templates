<?xml version="1.0"?>
<template
    format="5"
    revision="5"
    name="PicCacheUtils"
    minApi="9"
    minBuildApi="14"
    description="Creates a new template for PicCacheUtils">

    <category value="Utils" />
    <formfactor value="Mobile" />

	<parameter
            id="className"
            name="PicCacheUtils Name"
            type="string"
            constraints="class|unique|nonempty"
            default="PicCacheUtils"
            help="The name of the PicCacheUtils class to create"/>
	<parameter
            id="isBitmapUtils"
            name="PicCacheUtils Name"
            type="boolean"
            default="true"/>
		
    <!-- 128x128 thumbnails relative to template.xml -->
    <thumbs>
        <!-- default thumbnail is required -->
        <thumb>template_blank_activity.png</thumb>
    </thumbs>

    <globals file="globals.xml.ftl" />
    <execute file="recipe.xml.ftl" />

</template>
