<?xml version="1.0"?>
<template
    format="5"
    revision="5"
    name="ExpandableTextView"
    minApi="9"
    minBuildApi="14"
    description="Creates a new ExpandableTextView">

    <category value="Widget" />
    <formfactor value="Mobile" />

    <parameter
        id="className"
        name="ExpandableTextView Name"
        type="string"
        default="ExpandableTextView"
       />
   <parameter
        id="isUtils"
        name="Generate Utils File"
        type="boolean"
        default="true"
        help="If true, a Utils file will be generated" />
    <!-- 128x128 thumbnails relative to template.xml -->
    <thumbs>
        <!-- default thumbnail is required -->
        <thumb>template_blank_activity.png</thumb>
    </thumbs>

    <globals file="globals.xml.ftl" />
    <execute file="recipe.xml.ftl" />

</template>
